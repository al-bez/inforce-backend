{
  "name": "inforce",
  "version": "1.0.0",
  "description": "Inforce backend",
  "main": "server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "concurrently --kill-others \"nodemon ./dist/server.js\" \"yarn build:watch\"",
    "build": "tsc",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "build:watch": "tsc -w",
    "prettier": "prettier --check ./**/*.{js,ts}",
    "prettier:fix": "prettier --write ./**/*.{js,ts}",
    "eslint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet",
    "eslint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prepare": "husky install"
  },
  "author": "Alex Belz",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^17.0.17",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.2.1",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.1",
    "supertest": "^6.2.2",
    "typescript": "^4.5.5",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-mongo-sanitize": "^2.1.0",
    "@types/express-rate-limit": "^6.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0"
  }
}
